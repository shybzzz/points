{
  "compilerOptions": {
    "target": "ES6",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "removeComments": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "module": "commonjs",
    "types": ["node"]
  },
  "exclude": [
    "node_modules"
  ]
}